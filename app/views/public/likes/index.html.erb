 <div class="container">
   <div class="riw">
     <div class="col">
       <h6>いいねユーザー一覧</h6>
       <table class="table">
         <thead>
           <tr>
             <th>ユーザーname</th>
              <th>ユーザーネーム</th>
              <th>ユーザー画像</th>
              <th>プロフィール数</th>
              <th>フォロー数</th>
              <th>フォロワー数</th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <% @post.likes.each do |like| %>
            <% if like.user == current_user %>
              <% else %>
                <tr>
                  <td><%= like.user.full_name %></td>
                  <td><%= attachment_image_tag like.user, :image, :fill, 60, 60, fallback: "no_image.jpg"%></td>
                  <td><%= like.user.profiles.count %></td>
                  <td><%= like.user.followings.count %></td>
                  <td><%= like.user.followers.count %></td>
                  <td>
                    <%= link_to 'ユーザー詳細へ',public_user_path(like.user.id), class: "btn btn-success" %>
                  </td>
                </tr>
               <% end %>
            <% end %>
          </tbody>
       </table>
     </div>
   </div>
 </div>